{
  "proxies": [
    {
      "connect": {
        "from": "0.0.0.0:5000",
        "to": "0.0.0.0:3000"
      },
      "routes": [
        {
          "route": "/cars/*path",
          "open": {
            "method": ["GET"]
          },
          "allow": {
            "method": ["POST", "PUT", "DELETE"],
            "claims": [
              {"key": "role", "value": ["customer", "admin", "support"]},
              {"key": "foo", "value": ["bar"]}
            ]
          }
        },
        {
          "route": "/users/:user",
          "allow": {
            "method": ["GET", "POST"],
            "claims": [
              {"key": "role", "value": ["customer", "admin", "support"]}
            ]
          }
        }
      ]
    }
  ]
}
