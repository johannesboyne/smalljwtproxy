{
  "connect": {
    "from": "0.0.0.0:5000",
    "to": "127.0.0.1:8080"
  },
  "routes": [
    {
      "route": "/cars/*path",
      "allow": {
        "method": ["POST", "PUT", "DELETE"],
        "claims": [
          {"key": "role", "value": ["customer", "admin", "support"]},
          {"key": "foo", "value": ["bar"]}
        ]
      }
    },
{
      "route": "/cars/*path",
      "allow": {
        "method": ["GET"],
        "open": true,
        "claims": []
      }
    },
    {
      "route": "/config/*configs",
      "allow": {
        "method": ["GET"],
        "open": true,
        "claims": [
          {"key": "role", "value": ["customer", "admin", "support"]}
        ]
      }
    },
    {
      "route": "/config/*configs",
      "allow": {
        "method": ["PUT"],
        "claims": [
          {"key": "role", "value": ["customer", "admin", "support"]},
          {"key": "foo", "value": ["bar"]}
        ]
      }
    }
  ]
}
